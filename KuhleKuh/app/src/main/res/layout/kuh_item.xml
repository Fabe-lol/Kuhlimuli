<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginEnd="8dp"
    android:layout_marginStart="8dp"
    android:layout_marginTop="8dp">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="8dp"
        tools:ignore="MissingConstraints">
        <TextView
            android:id="@+id/text_view_ohrmarke"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="Ohrmarkennummer"
            android:textSize="25dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"/>

        <TextView
            android:id="@+id/text_view_et"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ET-Behandlung"
            android:maxLines="1"
            android:layout_below="@+id/text_view_ohrmarke"
            android:layout_alignParentStart="true"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/text_et_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nicht durchgeführt"
            android:maxLines="1"
            android:layout_below="@+id/text_view_ohrmarke"
            android:layout_alignParentRight="true"
            android:ellipsize="end"/>

        <TextView
            android:id="@+id/text_view_eut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Euterentzuendung"
            android:maxLines="1"
            android:layout_below="@+id/text_view_et"
            android:layout_alignParentStart="true"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/text_view_eut_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="von-bis oder bool"
            android:maxLines="1"
            android:layout_below="@+id/text_view_et"
            android:layout_alignParentRight="true"
            android:ellipsize="end"/>

        <TextView
            android:id="@+id/text_view_impf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Impfung Kaelberflechte"
            android:maxLines="1"
            android:layout_below="@+id/text_view_eut"
            android:layout_alignParentStart="true"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/text_view_impf_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nicht durchgeführt"
            android:maxLines="1"
            android:layout_below="@+id/text_view_eut"
            android:layout_alignParentRight="true"
            android:ellipsize="end"/>

        <TextView
            android:id="@+id/text_view_klaue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Klauenerkrankung"
            android:maxLines="1"
            android:layout_below="@+id/text_view_impf"
            android:layout_alignParentStart="true"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/text_view_klaue_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nicht durchgeführt"
            android:maxLines="1"
            android:layout_below="@+id/text_view_impf"
            android:layout_alignParentRight="true"
            android:ellipsize="end"/>

        <TextView
            android:id="@+id/text_view_nabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nabelerkrankung"
            android:maxLines="1"
            android:layout_below="@+id/text_view_klaue_date"
            android:layout_alignParentStart="true"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/text_view_nabel_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nicht durchgeführt"
            android:maxLines="1"
            android:layout_below="@+id/text_view_klaue_date"
            android:layout_alignParentRight="true"
            android:ellipsize="end"/>

        <TextView
            android:id="@+id/text_view_nachgeb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nachgeburtsverhaltung"
            android:maxLines="1"
            android:layout_below="@+id/text_view_nabel"
            android:layout_alignParentStart="true"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/text_view_nachgeb_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nicht durchgeführt"
            android:maxLines="1"
            android:layout_below="@+id/text_view_nabel"
            android:layout_alignParentRight="true"
            android:ellipsize="end"/>

        <TextView
            android:id="@+id/text_view_sonderb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Sonderbehandlung"
            android:maxLines="1"
            android:layout_below="@+id/text_view_nachgeb"
            android:layout_alignParentStart="true"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/text_view_sonderb_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nicht durchgeführt"
            android:maxLines="1"
            android:layout_below="@+id/text_view_nachgeb"
            android:layout_alignParentRight="true"
            android:ellipsize="end"/>

        <TextView
            android:id="@+id/text_view_trockenst"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Trockenstellen"
            android:maxLines="1"
            android:layout_below="@+id/text_view_sonderb"
            android:layout_alignParentStart="true"
            android:ellipsize="end"/>
        <TextView
            android:id="@+id/text_view_trockenst_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="nicht durchgeführt"
            android:maxLines="1"
            android:layout_below="@+id/text_view_sonderb"
            android:layout_alignParentRight="true"
            android:ellipsize="end"/>
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>